<?xml version="1.0" encoding="utf-8"?>


<cd.ex.leovs09.experiment_content_discavery.image_swipe.vertical.SwipeImageVerticalLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".image_swipe.vertical.SwipeVerticalActivity"
    tools:showIn="@layout/activity_swipe_vertical_image"
    xmlns:android="http://schemas.android.com/apk/res/android">

        <ImageView
            android:layout_width="60sp"
            android:layout_height="60sp"
            android:id="@+id/dislike"
            android:layout_centerHorizontal="true"
            android:src="@drawable/dislike"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10sp" />

        <ImageView
            android:layout_width="60sp"
            android:layout_height="60sp"
            android:id="@+id/like"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:src="@drawable/like"
            android:layout_marginBottom="10sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/alai_oli"
            android:clickable="true" />

</cd.ex.leovs09.experiment_content_discavery.image_swipe.vertical.SwipeImageVerticalLayout>
